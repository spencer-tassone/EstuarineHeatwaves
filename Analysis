  cur_mhw_time = data.frame(TestType = "Time",
                           Variable = "Avg.Decline",
                           Category = "Time",
                           slope = round(slope, 3),
                           p.val = round(p.val, 4))
  if( i == 1){
    mhw_time_decline = cur_mhw_time
  } else{
    mhw_time_decline = rbind(mhw_time_decline, cur_mhw_time)
  }
