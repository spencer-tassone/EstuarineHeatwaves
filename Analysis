round(((MHW_total_InRange/MHW_total_events)*100), digits = 0)
round(((lowDO_total_InRange/lowDO_total_MHWevents)*100), digits = 0)
round(((lowPH_total_InRange/lowPH_total_MHWevents)*100), digits = 0)

