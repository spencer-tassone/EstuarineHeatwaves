SI_Table1 <- left_join(SI_Table1, df_station_lowPH, by = "site")

SI_Table1$Order <- c(11,13,14,8,6,7,16,17,2,3,4,12,5,9,10,15,1)
SI_Table1 <- SI_Table1[,c(11,1:10)]
View(SI_Table1)
