df_station_lowDO[1,1] <- summary(LowDO_fitted_SumAnnualDuration_models$acesp)$coefficients[2,1]
df_station_lowDO[2,1] <- summary(LowDO_fitted_SumAnnualDuration_models$apaeb)$coefficients[2,1]
